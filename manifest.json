{
    "manifest_version": 2,
    "name": "Bitbucket JIRA Linker",
    "version": "0.2",
    "description": "Links commit messages to JIRA ticket",

    "content_scripts": [
        {
            "matches": ["https://bitbucket.org/buildingengines/*", "file:///*/test*.html"],
            "js": ["jquery-3.1.1.min.js", "content.js"]
        }
    ]
}
